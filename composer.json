{
    "name": "survos-sites/gist",
    "description": "A fast word-by-word translator using freedict dictionaries",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "ext-xmlreader": "*",
        "doctrine/dbal": "^4.3.2",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "easycorp/easyadmin-bundle": "^4.24.9",
        "inspector-apm/inspector-symfony": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.2",
        "skoro/stardict": "^0.2.1",
        "survos/core-bundle": "^1.6.44",
        "survos/deployment-bundle": "^1.6.44",
        "symfony/asset": "^7.3",
        "symfony/asset-mapper": "^7.3.2",
        "symfony/console": "^7.3.2",
        "symfony/doctrine-messenger": "^7.3.2",
        "symfony/dotenv": "^7.3.2",
        "symfony/expression-language": "^7.3.2",
        "symfony/filesystem": "^7.3.2",
        "symfony/flex": "^2.8.2",
        "symfony/form": "^7.3.2",
        "symfony/framework-bundle": "^7.3.2",
        "symfony/http-client": "^7.3.2",
        "symfony/intl": "^7.3.2",
        "symfony/mailer": "^7.3.2",
        "symfony/mime": "^7.3.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.3",
        "symfony/process": "^7.3",
        "symfony/property-access": "^7.3.2",
        "symfony/property-info": "^7.3.1",
        "symfony/runtime": "^7.3.1",
        "symfony/security-bundle": "^7.3.2",
        "symfony/serializer": "^7.3.2",
        "symfony/stimulus-bundle": "^2.29.2",
        "symfony/string": "^7.3.2",
        "symfony/translation": "^7.3.2",
        "symfony/twig-bundle": "^7.3.2",
        "symfony/ux-turbo": "^2.29.2",
        "symfony/validator": "^7.3.2",
        "symfony/web-link": "^7.3",
        "symfony/yaml": "^7.3.2",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/twig": "^2.12|^3.21.1",
        "wamania/php-stemmer": ">=4",
        "wapmorgan/unified-archive": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.3"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3.6",
        "symfony/browser-kit": "^7.3.2",
        "symfony/css-selector": "^7.3",
        "symfony/debug-bundle": "^7.3",
        "symfony/maker-bundle": "^1.64",
        "symfony/stopwatch": "^7.3",
        "symfony/web-profiler-bundle": "^7.3.2"
    }
}
